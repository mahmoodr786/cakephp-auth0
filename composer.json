    "authors": [
        {
            "name":"Mahmood Rahimi",
            "role":"Author",
            "homepage":"https://icingcms.org"
        }
    ],
    "license": "MIT",
    "support": {
        "source":"https://github.com/mahmoodr786/cakephp-auth0",
        "issues":"https://github.com/mahmoodr786/cakephp-auth0/issues"
    },
    "require": {
        "cakephp/cakephp": "~3.1",
        "auth0/auth0-php": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Mahmoodr786\\AuthZero\\": "src"
        }
    }
}
